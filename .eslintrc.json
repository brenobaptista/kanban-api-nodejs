{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "no-console": "off",
      "consistent-return": "off",
      "no-underscore-dangle": "off",
      "no-unused-vars": "off",
      "prefer-promise-reject-errors": "off"
    }
}